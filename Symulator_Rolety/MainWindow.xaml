        <ToggleButton x:Name="workModeBtn" Content="Auto Work" Margin="250,50,250,0" VerticalAlignment="Top" BorderBrush="Black" BorderThickness="3" FontWeight="Bold" Click="workModeBtn_Click">
            <ToggleButton.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="White" Offset="0"/>
                    <GradientStop Color="Red" Offset="1"/>
                </LinearGradientBrush>
            </ToggleButton.Background>
        </ToggleButton>
        <Slider x:Name="blindSlider" HorizontalAlignment="Left" Margin="350,0,0,20" VerticalAlignment="Bottom" Width="200" RenderTransformOrigin="0.525,-3.333" Background="#FFFF7400" BorderBrush="Black" Foreground="Red" d:LayoutOverrides="VerticalAlignment" ValueChanged="ControlBlinds" Maximum="100"/>
        <Label x:Name="Label1" Content="Manual control of blinds" HorizontalAlignment="Right" Margin="0,0,30,40" VerticalAlignment="Bottom" Width="224.48" FontSize="14.667" FontWeight="Bold" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        <Slider x:Name="lightSlider" HorizontalAlignment="Left" Margin="40,0,0,20" VerticalAlignment="Bottom" Width="200" Background="#FFFF7400" BorderBrush="Black" Foreground="Red" Maximum="100" ValueChanged="ControlLamps"/>
        <Label x:Name="Label2" Content="Control of light" HorizontalAlignment="Left" Margin="80,0,0,40" VerticalAlignment="Bottom" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="14.667" FontWeight="Bold"/>
        <ProgressBar x:Name="progressBar" Value="{Binding Value, ElementName=blindSlider}" Height="20" Width="200" Margin="195,0,195,75" VerticalAlignment="Bottom" BorderThickness="3" BorderBrush="White" Cursor="None" Background="White"/>
        <Label x:Name="Label3" Content="Progress of closing blind" Margin="206,0,0,95" VerticalAlignment="Bottom" FontSize="14.667" FontWeight="Bold" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" HorizontalAlignment="Left"/>
        <Rectangle x:Name="lightSensor1" HorizontalAlignment="Left" Height="32" Margin="91,128,0,0" Stroke="Black" VerticalAlignment="Top" Width="29" RadiusX="5" RadiusY="5" Fill="Red"/>
        <Rectangle x:Name="lightSensor2" HorizontalAlignment="Right" Height="32" Margin="0,128,90,0" RadiusY="5" RadiusX="5" Stroke="Black" VerticalAlignment="Top" Width="30" Fill="Red"/>
        <Button x:Name="foldBlindBtn" HorizontalAlignment="Right" Margin="0,215,40,0" VerticalAlignment="Top" Width="50" Click="FoldBlindBtn_Click" Height="65" BorderBrush="Black">
            <StackPanel>
                <Image Source="Data/up.jpg" />
            </StackPanel>
        </Button>
        <Button x:Name="unfoldBlinfBtn"  HorizontalAlignment="Right" Margin="0,280,40,0" VerticalAlignment="Top" Width="50" Click="UnfoldBlindBtnClick" Height="65" BorderBrush="Black">
            <StackPanel>
                <Image Source="Data/down.jpg" />
            </StackPanel>
        </Button>
    </Grid>
</Window>
